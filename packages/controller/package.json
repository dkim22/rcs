{
  "name": "@abb/controller",
  "version": "1.0.0",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "license": "MIT",
  "scripts": {
    "build": "rimraf ./dist && tsc",
    "introspect": "apollo-codegen introspect-schema http://localhost:4000 --output schema.json",
    "generate": "apollo-codegen generate src/**/*.tsx --schema schema.json --target ts-modern",
    "gen-types": "yarn introspect && yarn generate"
  },
  "devDependencies": {
    "@types/node": "^13.13.4",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.7",
    "apollo-codegen": "^0.19.1",
    "rimraf": "^3.0.2",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^5.0.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@apollo/client": "^3.0.0-beta.44",
    "@apollo/react-components": "^4.0.0-beta.1",
    "@apollo/react-hoc": "^4.0.0-beta.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  }
}
